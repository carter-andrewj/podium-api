{
  "name": "@carter_andrewj/podix",
  "version": "2.1.2",
  "description": "API library between the Podium social network and Radix DLT.",
  "main": "lib/podium.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "npx babel src --out-dir lib",
    "build-local": "npm run build && rm -rfv ../client/src/podix/* && cp -a lib/. ../client/src/podix/",
    "deploy": "npm publish --access=public",
    "deploy-patch": "npm version patch && npm publish --access=public",
    "deploy-minor": "npm version minor && npm publish --access=public",
    "deploy-major": "npm version major && npm publish --access=public"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "form-data": "^2.3.3",
    "immutable": "^4.0.0-rc.12",
    "node-fetch": "^2.3.0",
    "radixdlt": "^1.0.6-alpha"
  },
  "devDependencies": {
    "@babel/cli": "^7.2.3",
    "@babel/core": "^7.2.2",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.2.3"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ]
  }
}
